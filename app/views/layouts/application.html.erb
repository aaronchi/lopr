<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Sexy Younger You</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body class="<%= params[:controller] %>-layout">
    <div id="wrapper">
      <%= render 'header' %>
      <% if alert || notice %>
      <div id="flash"><%= alert %><%= notice %></div>
      <% end %>
      <%= yield %>
      <%= render 'footer' %>
      <%#= render 'tracking' if Rails.env.production? %> 
    </div>
  </body>
</html>