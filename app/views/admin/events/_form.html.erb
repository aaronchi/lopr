<%= simple_form_for resource, :url => form_resource_path do |f| %>
  <%= f.input :name %>
  <%= f.association :speaker %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :event_id, :label => 'TS eventID' %>
  <%= f.input :ia_id, :label => 'TS IAID' %>
  <%= f.input :description %>
  <div class="buttons">
  <%= f.submit %>
  </div>
<% end %>
